{
  "include": [
    "src/**/*.js",
    "src/**/*.jsx"
  ],
  "exclude": [
    "**/*.spec.js",
    "coverage",
    "dist",
    "test",
    "src/index.jsx"
  ],
  "extension": [".jsx"],
  "reporter": [
    "text-summary"
  ],
  "require": [
    "babel-register"
  ],
  "all": true,
  "instrument": false,
  "sourceMap": false
}
